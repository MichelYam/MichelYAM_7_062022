{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "babel-eslint"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "quotes": [
            "error", "single"
        ],
        "import/extensions": "off"
    }
}
